<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="text_view_small_normal_white_center_vertical_2dp_padding_left">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14dp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:paddingLeft">1dp</item>
    </style>

    <style name="text_view_small_normal_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="text_view_small_normal_white_padding_top_bottom">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="text_view_small_bold_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_view_large_bold_white">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="text_view_filter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textStyle">bold</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textSize">8dp</item>
    </style>

    <style name="text_view_number">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/number</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="text_view_left_login_register">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_info_row_left</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="edit_text_right_loggin_regiter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/bg_info_row_right</item>
        <item name="android:textSize">14dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="linear_layout_row_loggin_regiter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginTop">20dp</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>

    <style name="edit_text_small_normal_white_for_lay_out_loggin_regiter">
        <item name="android:layout_width">180dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:lines">1</item>
        <item name="android:maxLines">1</item>
        <item name="android:singleLine">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textSize">15dp</item>
        <item name="android:background">@drawable/bg_infobar_right_register</item>
    </style>

    <style name="button_authen_facebook_twitter">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold|italic</item>
        <item name="android:textSize">16dp</item>
        <item name="android:text">@string/text_view_sign_in</item>
    </style>

    <style name="button_login_register">
        <item name="android:layout_width">160dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/gradient_button_white</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">16dp</item>
    </style>

    <style name="linear_layout_high_light_selector">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginRight">20dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@drawable/highlight_selector</item>
    </style>

    <style name="linear_layout_filter">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">3dp</item>
        <item name="android:layout_marginBottom">3dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>

    <style name="Theme.Transparent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

</resources>