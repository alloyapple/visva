<?xml version="1.0" encoding="UTF-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:background="@drawable/bg_more_stories">
	<ImageButton android:id="@+id/btn_doubting_dasha"
		android:layout_width="200dp" android:layout_height="18dp"
		android:layout_marginRight="130dp" android:layout_alignParentRight="true"
		android:layout_marginTop="110dp" android:background="@drawable/btn_drawable_doubting_dasha" />
	<ImageButton android:id="@+id/btn_mommy_love_you"
		android:layout_width="200dp" android:layout_height="34dp"
		android:layout_alignParentRight="true" android:layout_below="@id/btn_doubting_dasha"
		android:layout_marginTop="40dp" android:layout_marginRight="120dp"
		android:background="@drawable/btn_drawable_mommy_love_you" />
	<ImageButton android:id="@+id/btn_booger_town"
		android:layout_width="180dp" android:layout_height="19dp"
		android:layout_alignParentRight="true" android:layout_below="@id/btn_mommy_love_you"
		android:layout_marginTop="40dp" android:layout_marginRight="130dp"
		android:background="@drawable/btn_drawable_booger_town" />
	<ImageButton android:id="@+id/btn_house"
		android:layout_width="36dp" android:layout_height="36dp"
		android:layout_alignParentRight="true"
		android:layout_alignParentBottom="true" android:layout_marginBottom="10dp"
		android:layout_marginRight="10dp" android:background="@drawable/btn_house" />
</RelativeLayout>